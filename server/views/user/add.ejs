<%-include ../partials/header.ejs%>
<%-include ../partials/main_nav.ejs%>

 <!--Main Content-->
   <!-- ======= Add User Section ======= -->
   <section id="AddUser" class="AddUser d-flex flex-column align-items-center justify-content-center">
    <h1><%= title %></h1>
    <div class="container">
        <div class="row" style="min-height: 500px;">
            <div class="offset-md-3 col-md-6">
    
        
            
            <form action="/buisiness-contact/add" method="POST" class="form">
                <div class="form-group">
                    <labe for="nameTextField">Name</labe>
                    <input type="text" class="form-control" 
                    id="nameTextField" 
                    placeholder="name" 
                    name="name" required>
                </div>
    
                <div class="form-group">
                    <labe for="emailTextField">Email</labe>
                    <input type="text" class="form-control" 
                    id="emailTextField" 
                    placeholder="email" 
                    name="email" required>
                </div>
    
                <div class="form-group">
                    <labe for="contactTextField">Contact</labe>
                    <input type="text" class="form-control" 
                    id="contactTextField" 
                    placeholder="phone" 
                    name="phone" required>
                </div>
                <button class="btn btn-primary" type="submit" id="addSubmit">
                    <i class="fas fa-edit"></i> Submit
                </button>
                <a href="/buisiness-contact" class="btn btn-warning" id="addCancel"><i class="fas fa-undo"></i>Cancel</a>
    
                
            </form>


            </div>
        </div>

      

    </div>
  </section><!-- End Services Section -->




<%-include ../partials/footer.ejs%>
<%-include ../partials/footer_nav.ejs%>